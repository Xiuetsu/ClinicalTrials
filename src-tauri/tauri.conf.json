{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "trialtracker",
  "version": "0.1.0",
  "identifier": "com.trialtracker.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "trialtracker",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {},
  "plugins": {}
}
